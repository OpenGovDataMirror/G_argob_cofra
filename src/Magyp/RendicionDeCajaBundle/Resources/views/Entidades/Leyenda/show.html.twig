<h1>Leyenda</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Leyenda</th>
            <td>{{ entity.leyenda }}</td>
        </tr>
        <tr>
            <th>Anio</th>
            <td>{{ entity.anio|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('entidades_leyenda') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('entidades_leyenda_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('entidades_leyenda_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
