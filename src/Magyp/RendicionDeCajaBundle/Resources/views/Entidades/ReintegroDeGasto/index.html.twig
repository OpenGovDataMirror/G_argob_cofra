<h1>ReintegroDeGasto list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Expediente</th>
            <th>Nota</th>
            <th>Periodoinicial</th>
            <th>Periodofinal</th>
            <th>Total</th>
            <th>Fuentefinanciamiento</th>
            <th>Borrado</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('sistema_entidades_reintegrodegasto_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.expediente }}</td>
            <td>{{ entity.nota }}</td>
            <td>{% if entity.periodoinicial %}{{ entity.periodoinicial|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{% if entity.periodofinal %}{{ entity.periodofinal|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{{ entity.total }}</td>
            <td>{{ entity.fuentefinanciamiento }}</td>
            <td>{{ entity.borrado }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('sistema_entidades_reintegrodegasto_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('sistema_entidades_reintegrodegasto_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('sistema_entidades_reintegrodegasto_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
